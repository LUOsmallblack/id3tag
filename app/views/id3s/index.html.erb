<p id="notice"><%= notice %></p>

<h1>Listing Id3s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Album</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @id3s.each do |id3| %>
      <tr>
        <td><%= id3.title %></td>
        <td><%= id3.artist %></td>
        <td><%= id3.album %></td>
        <td><%= link_to 'Show', id3 %></td>
        <td><%= link_to 'Edit', edit_id3_path(id3) %></td>
        <td><%= link_to 'Destroy', id3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Id3', new_id3_path %>
